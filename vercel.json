{
  "rewrites": [
    {
      "source": "/_proxy/bound/:adcode",
      "destination": "https://geo.datav.aliyun.com/areas_v3/bound/:adcode_full.json"
    },
    {
      "source": "/_proxy/gaode",
      "destination": "https://webst01.is.autonavi.com/appmaptile"
    }
  ],
  "headers": [
    {
      "source": "/_proxy/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400, stale-while-revalidate=604800"
        }
      ]
    }
  ]
}
